function fibonacci(limit is int) {
    if (limit < 0) {
        return
    }
    a, b, aux is int
    a = 0
    b = 1
    textOut(a)
    repeat while (a < limit) {
        aux = a
        a = b
        b = aux + b
        textOut(", " + a)
    }
}

@isEntryPoint
function main() is int {
    limit is int
    limit = lineIn() as int
    fibonacci(limit)
    return 0
}
